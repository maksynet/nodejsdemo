<% var usernamex = null,
   bio = null,
   sex = null,
   userimgPath = null;

userinfo.forEach(function(inf) {
 usernamex = inf.adsoyad,
 bio = inf.bio,
 sex = inf.cinsiyet,
 userimgPath = inf.pathPhoto
});
%>
<div id="top">
  <div class="container">
    <div class="w50">
      <h3 class="logs"></h3>
    </div>
    <div class="w50">
      <div class="rightPull">
         <div class="profileMini">
           <a href="/profile">
             <img style="background: url('/public/img/users/<%=userimgPath%>') no-repeat center;" class="circle bgPic" width="50" height="50" alt=""/>
             <span><%=usernamex%></span>
           </a>
         </div>
      </div>
    </div>
  </div>
</div>
<section class="w100 content">
  <div class="container">

    <div class="wrapFlex">

      <div class="flex-col-2 leftWrap asideLeft leftWrapBox">
        <div class="section">
            <h5 id="userName"><%=usernamex%></h5>
          <ul>
            <li><a href="/profile">Profil</a></li>
            <li><a href="/config">Ayarlar</a></li>
            <li><a href="/rooms">Sohbet</a></li>
            <li><a href="/logout">Çıkış Yap</a></li>
          </ul>
        </div>
        <div class="section">
          <h5>Takip Ettiklerin</h5>
          <ul class="roomS">
              <% myfollowers.forEach(function(tval){ %>
                <li><a href="/<%=tval.tedilen%>"><span><%=tval.tedilen%></span>  <i class="fa fa-send-o"></i></a></li>
              <% }); %>
          </ul>
        </div>

        <div class="section helpBlock">
          <h5>Yardım</h5>
          <ul></ul>
        </div>
      </div>
      <div class="flex-col-9 leftWrap mainCenter">
        <article class="wrapChatContent">

          <div class="userProfileInformation">

            <a href="/public/img/users/<%=userimgPath%>" class="popupImage">
              <img style="background: url('/public/img/users/<%=userimgPath%>') no-repeat center;" class="circle userPic bgPic" alt="<%=usernamex%>"/>
            </a>
            <span class="userNameX"><%=usernamex%></span>
            <span class="userAgeX"><%=sex%></span>
            <span class="userDescp"><%=bio%></span>
           </div>

           <div class="altBlock">
            <span class="userFollowX"><a href="/config">Profili Düzenle</a></span>
            <span class="userFollowX followers"><a href="#">246 Takipçi</a></span>
            <span class="userFollowX follow"><a href="#">124 Takip</a></span>
           </div>


        </article>
      </div>
    </div>

  </div>
</section>
